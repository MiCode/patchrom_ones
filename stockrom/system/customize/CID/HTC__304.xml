    <module name="deviceData">
      <function>
        <set name="single">
          <item name="device_name">HTC One S</item>
        </set>
      </function>
    </module>
    <module name="deviceData1">
      <function>
        <set name="single">
          <item name="sw_number">NA</item>
        </set>
      </function>
    </module>
    <module name="Settings">
      <function name="screen_timeout">
        <set name="plenty">
          <item name="value1">15000</item>
          <item name="value2">30000</item>
          <item name="value3">45000</item>
          <item name="value4">60000</item>
          <item name="value5">120000</item>
          <item name="value6">600000</item>
          <item name="value7">-1</item>
        </set>
      </function>
    </module>
    <module name="defaultAutoTime">
      <function>
        <set name="single">
          <item name="default">0</item>
        </set>
      </function>
    </module>
    <module name="defaultTime1224">
      <function>
        <set name="single">
          <item name="default">24</item>
        </set>
      </function>
    </module>
    <module name="defaultTimeZone">
      <function>
        <set name="single">
          <item name="default">Europe/Brussels</item>
        </set>
      </function>
    </module>
    <module name="defaultAutoTimeZone">
      <function>
        <set name="single">
          <item name="default">0</item>
        </set>
      </function>
    </module>
